	DEC     A
	ld      de,NOZ80
	jp      pe,MSG

	ld      c,12
	call    BDOS
	exx
	ld      a,h
	cp      0ABh
	ld      de,BADSYS       ; проверка BEST DOS
	jp      nz,MSG
