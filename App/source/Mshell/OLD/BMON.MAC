;
; здесь встроить проверку занятости памяти и ее резервиро-
; вание для монитора
;
	LD      SP,STACK
	ld      hl,WORKMON      ; место Монитора в доп. MEM
	ld      a,0             ; номер банки в доп. MEM
	exx
	ex      af,af'
	ld      hl,DATAMON      ; коды Монитора
	ld      a,(MARKER)      ; банк системы
	ld      bc,EDATA-DATAMON; длина Монитора
	call    Bldir           ; переслать Монитор в доп.
				; MEM
	DI
	ld      a,0
	ld      hl,WORKMON
	jp      Bjump
MSG:    ld      c,9
	jp      BDOS

